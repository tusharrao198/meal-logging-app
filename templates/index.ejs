<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<!-- <link
			rel="icon"
			type="image/x-icon"
			href="images/xpecto-logo-header.png"
		/> -->
		<link rel="stylesheet" href="styles/style.css" />
		<link rel="stylesheet" href="styles/home-parallax.css" />

		<!-- -----FONT AWESOME ICONS----- -->
		<script
			src="https://kit.fontawesome.com/a1542a4884.js"
			crossorigin="anonymous"
		></script>

		<title>Meal | APP</title>
	</head>

	<body>
		<!-- -----HEADER INCLUDING LOGO AND NAVBAR----- -->
		<header class="header-glass active">
			<div>
				<img src="images/xpecto-logo-header.png" alt="" />
				<span>MEAL-LOG</span>
			</div>
			<nav>
				<a id="" href="#home">Home</a>
				<a id="" href="#about">About Us</a>
				<!-- <a id="" href="#contact">Contact Us</a> -->
				<!-- <a id="faq" href="">Faq</a> -->

				<% if(!authenticated){ %>
				<a id="sign" href="/auth/google"
					><i class="fa-brands fa-google"></i> SignIn</a
				>
				<% }else{ %>
				<a id="sign" href="/auth/logout"
					><i class="fa-brands fa-google"></i> Logout</a
				>
				<% } %>
			</nav>
			<div class="hamburger">
				<img class="toggle" src="images/menu_open.svg" alt="" />
			</div>
		</header>
		.
		<!-- -----HEADER END----- -->

		<main>
			<!-- -----HOME SECTION WITH PARALLAX EFFECT----- -->
			<section class="hero" id="home">
				<!-- <h1 id="text" class="translateX" data-speed="-5">MEAL APP</h1> -->
				<!-- <a href="#" id="btn">Explore</a> -->
			</section>
			<!-- -----HOME SECTION END----- -->



				<!-- -----SOCIAL HANDLES ICONS-----  -->
				<div class="social">
					<div>
						<a href="#">
							<i class="fa-brands fa-facebook-square fa-2x"></i>
						</a>
					</div>
					<div>
						<a href="#">
							<i class="fa-brands fa-twitter-square fa-2x"></i>
						</a>
					</div>
					<div>
						<a href="#">
							<i class="fa-brands fa-linkedin fa-2x"></i>
						</a>
					</div>
					<div>
						<a href="#">
							<i class="fa-brands fa-instagram-square fa-2x"></i>
						</a>
					</div>
					<div>
						<a href="#">
							<i class="fa-brands fa-youtube-square fa-2x"></i>
						</a>
					</div>
				</div>
				<!-- -----ICONS END----- -->
			</section>
			<!-- -----CONTACT SECTION END----- -->

		</main>
		<footer></footer>

		<script src="scripts/script.js"></script>
	</body>
</html>
